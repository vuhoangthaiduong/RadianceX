<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.radiancex">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".screen.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".screen.RegisterActivity">
        </activity>
        <activity android:name=".screen.MainActivity"></activity>
        <activity
            android:name=".screen.AddNewEntryActivity"
            android:parentActivityName=".screen.MainActivity" />
        <activity
            android:name=".screen.browse.BrowseActivity"
            android:parentActivityName=".screen.MainActivity" />
        <activity
            android:name=".screen.LearnActivity"
            android:parentActivityName=".screen.MainActivity" />
    </application>

</manifest>